from time import sleep

print('\033[33mContagem Regressiva...\033[m')

for c in range(10, 0, -1):
    print(c)
    sleep(1)

for c in range(0, 3):
    print('\033[33m*\033[m\033[36m*\033[m'*20)
    print('\033[33mPOOW\033[m ! \033[36mPOOW\033[m ! \033[33mPOOW\033[m!')
    sleep(0.4)

print('\033[33m*\033[m\033[36m*\033[m'*20)
print('\033[36mFeliz Ano Novo !\033[m')
